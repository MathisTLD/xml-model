{
  "name": "xml-model",
  "version": "1.3.1",
  "description": "allows transparent XML <-> Object conversion in typescript",
  "license": "MIT",
  "author": "MathisTLD",
  "repository": "git@github.com:MathisTLD/xml-model.git",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./vite": {
      "types": "./vite/dist/index.d.ts",
      "import": "./vite/dist/index.js"
    }
  },
  "scripts": {
    "preversion": "npm test -- --run",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "prepare": "husky"
  },
  "dependencies": {
    "typescript-rtti": "0.9.6",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "*",
    "@types/node": "^24.10.11",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "marmotte": "^0.1.4",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "reflect-metadata": "^0.1.14",
    "typescript": "*",
    "vite": "*",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "typescript": "~5.1.6",
    "vite": "^7.3.1"
  },
  "peerDependenciesMeta": {
    "@rollup/plugin-typescript": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "lint-staged": {
    "*": "oxfmt --no-error-on-unmatched-pattern"
  }
}
