{
  "name": "xml-model",
  "version": "0.2.7",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "author": "MathisTLD",
  "license": "MIT",
  "scripts": {
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "test": "npm run lint && mocha -r ./register-ts-node --colors src/**/*.spec.ts",
    "build": "run-p build:*",
    "build:main": "rimraf build/main && ttsc -p tsconfig.json",
    "build:module": "rimraf build/module && ttsc -p tsconfig.module.json",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "typescript-rtti": "^0.8.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/expect": "^24.3.0",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chai": "^4.3.6",
    "eslint": "^8.18.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.8.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4"
  },
  "files": [
    "build/main/**",
    "build/module/**",
    "!**/*.spec.*",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "description": "allows transparent XML <-> Object conversion in typescript",
  "repository": "git@github.com:MathisTLD/xml-model.git"
}
